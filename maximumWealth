class Solution {
    public int maximumWealth(int[][] accounts) {
        int m = accounts.length;
        int result = 0;
        for (int i=0;i<m;i++){
            int n = accounts[i].length;
            int wealth = 0;
            for (int j=0;j<n;j++){
                wealth += accounts[i][j];
            }
            result = Math.max(result,wealth);
        }
        return result;
    }
}
