class Solution {
    public int heightChecker(int[] heights) {
        int heightChecker=0;
        int[] expected = Arrays.copyOf(heights,heights.length);
        Arrays.sort(expected);
        for(int i=0;i<heights.length;i++){
            if(expected[i]!=heights[i]){
                heightChecker++;
            }
        }
        return heightChecker;
    }
}
