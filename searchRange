class Solution {
    public int[] searchRange(int[] nums, int target) {
        int[] ans = {-1,-1};
        for(int i=0;i<nums.length;i++){
            if(nums[i]==target && ans[0]==-1){
                ans[0]=i;
            }
            if(nums[nums.length-1-i]==target && ans[1]==-1){
                ans[1]=nums.length-1-i;
            }
            if(ans[0]!=-1 && ans[1]!=-1){
                break;
            }
        }
        return ans;
    }
}
