class Solution {
    public int tribonacci(int n) {
        if(n<2){
            return n;
        }
        if(n==2){
            return 1;
        }
        int firstNo = 0;
        int secondNo = 1;
        int thirdNo = 1;
        for(int i=1;i<=n;i++){
            int fourthNo = firstNo+secondNo+thirdNo;
            firstNo = secondNo;
            secondNo = thirdNo;
            thirdNo = fourthNo;
        }
        return firstNo;
  }
}
